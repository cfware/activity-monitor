{
	"name": "@cfware/activity-monitor",
	"version": "0.1.0",
	"description": "Browser activity monitor",
	"main": "activity-monitor.js",
	"scripts": {
		"release": "standard-version --sign",
		"test": "xo && nyc ava -v"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"files": [
		"activity-monitor.js"
	],
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/activity-monitor.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/activity-monitor/issues"
	},
	"homepage": "https://github.com/cfware/activity-monitor#readme",
	"dependencies": {
		"@cfware/add-event-listener": "^0.1.0",
		"@cfware/callback-array-once": "^1.0.0",
		"@cfware/cleanable-interval": "^0.1.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/plugin-transform-modules-commonjs": "^7.4.0",
		"@babel/register": "^7.4.0",
		"@cfware/eslint-config-browser": "^0.2.1",
		"ava": "^1.3.1",
		"babel-plugin-istanbul": "^5.1.1",
		"nyc": "^13.3.0",
		"p-series": "^2.0.0",
		"standard-version": "^5.0.2",
		"xo": "^0.24.0"
	},
	"nyc": {
		"all": true,
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"sourceMap": false,
		"instrument": false,
		"require": [
			"esm",
			"@babel/register"
		]
	},
	"xo": {
		"parser": "babel-eslint",
		"overrides": [
			{
				"files": [
					"activity-monitor.js"
				],
				"extends": "@cfware/eslint-config-browser"
			}
		]
	}
}
